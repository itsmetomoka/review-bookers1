<div class="container">
	<div class ="row">
		<h2><strong>Editing Book </strong></h2>
	</div>
	<div class="m-2">
		<%= form_for(@book) do |f| %>
			<h4>Title</h4>
		<%= f.text_field :title %>
	</div>
	<div class="m-2">
		<h4>Body</h4>
		<%= f.text_area :body %>
	</div>
	<div class="m-1">
		<%= f.submit "Update Book" %>
	<% end %>
	</div>
</div>
